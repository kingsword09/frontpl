{
  "name": "frontpl",
  "version": "0.3.0",
  "description": "Interactive CLI to scaffold standardized frontend project templates.",
  "keywords": [
    "cli",
    "frontend",
    "generator",
    "scaffold",
    "template"
  ],
  "homepage": "https://github.com/kingsword09/frontpl#readme",
  "bugs": {
    "url": "https://github.com/kingsword09/frontpl/issues"
  },
  "license": "MIT",
  "author": "Kingsword kingsword09 <kingsword09@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kingsword09/frontpl.git"
  },
  "bin": {
    "frontpl": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "start": "node dist/cli.mjs",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "oxlint --type-aware --type-check",
    "lint:fix": "oxlint --type-aware --type-check --fix",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "fmt": "pnpm run format",
    "fmt:check": "pnpm run format:check",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "tiny-bin": "^2.0.0"
  },
  "devDependencies": {
    "@kingsword/lint-config": "^0.1.1",
    "@types/node": "^25.0.10",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "oxlint-tsgolint": "^0.11.1",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.28.1"
}
